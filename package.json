{
    "name": "@optum/openid-client-server",
    "version": "1.0.4",
    "description": "An OpenId Relying Party (RP, Client) application server.",
    "license": "Apache-2.0",
    "repository": "https://github.com/optum/openid-client-server",
    "engines": {
        "node": ">=8"
    },
    "contributors": [
        {
            "name": "seesharpguy",
            "email": "ryan.sites@optum.com",
            "url": "https://github.com/seesharpguy"
        },
        {
            "name": "lxghtless",
            "email": "lxghtl3ss@gmail.com",
            "url": "https://github.com/lxghtless"
        }
    ],
    "scripts": {
        "build": "eslint \"src/**/*.ts\" && tsc -p src/tsconfig.json",
        "dev": "nodemon --exec ./run.sh",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "release": "release-it --no-npm.publish",
        "pretest": "eslint \"src/**/*.ts\"",
        "test": "cross-env TS_NODE_FILES=true intern",
        "clean": "rimraf dist rig/app/.next coverage"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "main": "dist/index.js",
    "files": [
        "dist",
        "src/**/*.ts",
        "src/options-schema.json"
    ],
    "keywords": [
        "openid",
        "openid-client",
        "web-app-server"
    ],
    "dependencies": {
        "ajv": "^6.12.6",
        "crypto-random-string": "^3.3.0",
        "fastify-http-proxy": "^4.0.4",
        "fastify-plugin": "^3.0.0",
        "fastify-secure-session": "^2.3.0",
        "openid-client": "^4.2.0",
        "path-to-regexp": "^6.2.0",
        "ramda": "^0.27.1"
    },
    "devDependencies": {
        "@types/node": "^14.14.0",
        "@types/ramda": "^0.27.29",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "babel-plugin-styled-components": "^1.11.1",
        "cross-env": "^7.0.2",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.13.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-sonarjs": "^0.5.0",
        "eslint-plugin-unicorn": "^23.0.0",
        "fastify": "^3.7.0",
        "husky": "^4.3.0",
        "intern": "^4.8.8",
        "next": "^9.5.5",
        "nodemon": "^2.0.6",
        "prettier": "^2.1.2",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-is": "^16.13.1",
        "rimraf": "^3.0.2",
        "styled-components": "^5.2.0",
        "swr": "^0.3.6",
        "typescript": "^4.0.3"
    },
    "peerDependencies": {
        "fastify": ">=3.3.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    }
}
